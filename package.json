{
  "name": "token-price-service",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "cross-env NODE_ENV=localhost nest start",
    "start:dev": "cross-env NODE_ENV=localhost nest start --watch",
    "start:debug": "cross-env NODE_ENV=localhost nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --detectOpenHandles",
    "migration:generate": "drizzle-kit generate --config drizzle.config.ts",
    "migration:run": "cross-env NODE_ENV=localhost drizzle-kit migrate --config drizzle.config.ts",
    "create_token": "cross-env NODE_ENV=localhost ts-node scripts/add_token.ts"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/fastify": "^6.13.0",
    "@bull-board/nestjs": "^6.13.0",
    "@bull-board/ui": "^6.13.0",
    "@fastify/basic-auth": "^6.2.0",
    "@fastify/static": "^8.0.0",
    "@knaadh/nestjs-drizzle-pg": "^1.2.0",
    "@knaadh/nestjs-drizzle-postgres": "^1.2.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-cls/transactional": "^3.1.0",
    "@nestjs-cls/transactional-adapter-drizzle-orm": "^1.2.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "bullmq": "^5.59.0",
    "cross-env": "^10.1.0",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "fastify": "5.4.0",
    "ioredis": "^5.8.0",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "nestjs-cls": "^6.0.1",
    "nestjs-zod": "^5.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typescript-memoize": "^1.1.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "drizzle-kit": "^0.31.5",
    "drizzle-seed": "^0.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "moment": "^2.30.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "testcontainers": "^10.28.0",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "zocker": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
